#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
#include<stdlib.h>
#include<string.h>
#include<assert.h>
char* my_strcpy(char* s1, const char* s2)
{
	assert(s1 && s2);
	char* ret = s1;
	while (*s1++ = *s2++)
	{
		;
	}
	return ret;
}
int main()
{
	char str1[] = "abcde";
	char str2[] = "fgh";
	//strcpy(str1, str2);
	puts(my_strcpy(str1,str2));
}
